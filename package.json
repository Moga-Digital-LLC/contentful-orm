{
  "name": "contentful-orm",
  "version": "0.1.2",
  "description": "A TypeScript-first ORM for Contentful CMS that enables a code-first approach to content modeling",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "contentful-orm": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "cli": "ts-node src/cli/index.ts"
  },
  "keywords": [
    "contentful",
    "orm",
    "typescript",
    "cms",
    "content-modeling"
  ],
  "author": "Jeff Lee",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "contentful-management": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "commander": "^9.0.0",
    "dotenv": "^16.0.0",
    "glob": "11.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
